[ignore]
shells/browser/chrome/build/*
shells/browser/firefox/build/*
shells/dev/build/*

[declarations]
<PROJECT_ROOT>/node_modules/graphql

[include]

[libs]
./flow.js

[lints]

[options]
module.name_mapper='^src' ->'<PROJECT_ROOT>/src'
